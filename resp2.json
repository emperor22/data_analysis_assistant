      "steps": [
        {
          "function": "filter",
          "column_name": "loan_status",
          "operator": "in",
          "values": ["fully paid"]
        },
        {
          "function": "groupby",
          "columns_to_group_by": ["income_bracket", "loan_size_category"],
          "columns_to_aggregate": ["id"],
          "calculation": ["count"]
        },
        {
          "function": "get_top_or_bottom_N_entries",
