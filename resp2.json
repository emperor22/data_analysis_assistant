  "common_column_combination": [
    {
      "name": "debt_to_income_ratio",
      "formula": "loan_amnt / annual_inc"
    },
    {
      "name": "interest_to_principal_ratio",
      "formula": "total_pymnt / loan_amnt"
    },
    {
      "name": "monthly_income",
      "formula": "annual_inc / 12"
    },
    {
      "name": "installment_to_income_ratio",
      "formula": "installment / (annual_inc / 12)"
    }
  ],
  "common_column_cleaning_or_transformation": [
    {
      "name": "loan_size_category",
      "description": "Categorize loans by amount into small, medium, and large",
      "formula": "MAP_RANGE(loan_amnt, [{'0-5000': 'Small'}, {'5001-15000': 'Medium'}, {'15001-inf': 'Large'}])"
    },
    {
      "name": "income_bracket",
      "description": "Group annual income into brackets for analysis",
      "formula": "MAP_RANGE(annual_amnt, [{'0-30000': 'Low'}, {'30001-60000': 'Medium'}, {'60001-100000': 'High'}, {'100001-inf': 'Very_High'}])"
    },
    {
      "name": "risk_category",
      "description": "Categorize loans by interest rate risk level",
      "formula": "MAP_RANGE(int_rate, [{'0-10': 'Low_Risk'}, {'10.01-15': 'Medium_Risk'}, {'15.01-inf': 'High_Risk'}])"
    },
    {
      "name": "loan_term_category",
      "description": "Convert numerical term to categorical short/long term",
      "formula": "MAP(term, {'36': 'Short_Term', '60': 'Long_Term'})"
    }
  ],
  "common_tasks": [
    {
      "name": "Analyze default rates by loan purpose",
      "task_id": 1,
      "description": "Calculate the percentage of charged off loans for each loan purpose category",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": ["purpose"],
          "columns_to_aggregate": ["loan_status"],
          "calculation": ["count"]
        },
        {
          "function": "get_proportion",
          "column_name": ["loan_status"],
          "values": ["charged off"]
        }
      ],
      "score": 0.95
    },
    {
      "name": "Compare average interest rates by home ownership status",
      "task_id": 2,
      "description": "Calculate mean interest rates for different home ownership categories",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": ["home_ownership"],
          "columns_to_aggregate": ["int_rate"],
          "calculation": ["mean"]
        }
      ],
      "score": 0.9
    },
    {
      "name": "Identify top 10 highest loan amounts by verified status",
      "task_id": 3,
      "description": "Find the largest loans grouped by income verification status",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": ["verification_status"],
          "columns_to_aggregate": ["loan_amnt"],
          "calculation": ["max"]
        },
        {
          "function": "get_top_or_bottom_N_entries",
          "sort_by_column_name": "loan_amnt",
          "order": "top",
          "number_of_entries": 10,
          "return_columns": ["verification_status", "loan_amnt"]
        }
      ],
      "score": 0.85
    },
    {
      "name": "Analyze loan performance by term length",
      "task_id": 4,
      "description": "Compare default rates between 36-month and 60-month loans",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": ["term", "loan_status"],
          "columns_to_aggregate": ["id"],
          "calculation": ["count"]
        },
        {
          "function": "get_proportion",
          "column_name": ["loan_status"],
          "values": ["charged off"]
        }
      ],
      "score": 0.92
    },
    {
      "name": "Calculate average annual income by loan purpose",
      "task_id": 5,
      "description": "Determine the mean income of borrowers for different loan purposes",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": ["purpose"],
          "columns_to_aggregate": ["annual_inc"],
          "calculation": ["mean"]
        }
      ],
      "score": 0.88
    },
    {
      "name": "Find loans with highest debt-to-income ratio",
      "task_id": 6,
      "description": "Identify loans where the loan amount represents a high percentage of annual income",
      "steps": [
        {
          "function": "filter",
          "column_name": "loan_amnt",
          "operator": ">",
          "values": [10000]
        },
        {
          "function": "filter",
          "column_name": "annual_inc",
          "operator": "<",
          "values": [50000]
        },
        {
          "function": "get_top_or_bottom_N_entries",
          "sort_by_column_name": "debt_to_income_ratio",
          "order": "top",
          "number_of_entries": 15,
          "return_columns": ["id", "loan_amnt", "annual_inc", "debt_to_income_ratio"]
        }
      ],
      "score": 0.87
    },
    {
      "name": "Analyze installment amounts by loan status",
      "task_id": 7,
      "description": "Compare monthly payment amounts across different loan outcomes",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": ["loan_status"],
          "columns_to_aggregate": ["installment"],
          "calculation": ["mean", "median"]
        }
      ],
      "score": 0.89
    },
    {
      "name": "Identify most common loan purposes for high-risk borrowers",
      "task_id": 8,
      "description": "Find loan purposes with highest interest rates above 15%",
      "steps": [
        {
          "function": "filter",
          "column_name": "int_rate",
          "operator": ">",
          "values": [15]
        },
        {
          "function": "groupby",
          "columns_to_group_by": ["purpose"],
          "columns_to_aggregate": ["id"],
          "calculation": ["count"]
        },
        {
          "function": "get_top_or_bottom_N_entries",
          "sort_by_column_name": "id",
          "order": "top",
          "number_of_entries": 5,
          "return_columns": ["purpose", "id"]
        }
      ],
      "score": 0.86
    },
    {
      "name": "Compare total payments by verification status",
      "task_id": 9,
      "description": "Analyze if verified income leads to better repayment outcomes",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": ["verification_status"],
          "columns_to_aggregate": ["total_pymnt"],
          "calculation": ["mean", "sum"]
        }
      ],
      "score": 0.84
    },
    {
      "name": "Analyze loan size distribution by home ownership",
      "task_id": 10,
      "description": "Compare loan amounts across different home ownership categories",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": ["home_ownership"],
          "columns_to_aggregate": ["loan_amnt"],
          "calculation": ["min", "max", "mean", "median"]
        }
      ],
      "score": 0.88
    },
    {
      "name": "Find correlation between interest rates and default rates",
      "task_id": 11,
      "description": "Analyze if higher interest rates correlate with higher charge-off rates",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": ["risk_category"],
          "columns_to_aggregate": ["loan_status"],
          "calculation": ["count"]
        },
        {
          "function": "get_proportion",
          "column_name": ["loan_status"],
          "values": ["charged off"]
        }
      ],
      "score": 0.91
    },
    {
