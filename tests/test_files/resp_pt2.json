{
    "common_tasks": [
        {
            "name": "Content Distribution by Country and Type",
            "task_id": 1,
            "description": "Analyze the count of movies and series available in each country to understand regional content focus.",
            "steps": [
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "country_availability",
                        "series_or_movie"
                    ],
                    "columns_to_aggregate": [
                        "series_or_movie"
                    ],
                    "calculation": [
                        "count"
                    ]
                }
            ],
            "score": "high"
        },
        {
            "name": "Identify Top Genres by Hidden Gem Score",
            "task_id": 2,
            "description": "Determine the top 5 genres with the highest average 'hidden gem scores', indicating potentially undervalued content categories.",
            "steps": [
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "genre"
                    ],
                    "columns_to_aggregate": [
                        "hidden_gem_score"
                    ],
                    "calculation": [
                        "mean"
                    ]
                },
                {
                    "function": "get_top_or_bottom_N_entries",
                    "sort_by_column_name": "hidden_gem_score",
                    "order": "top",
                    "number_of_entries": 5,
                    "return_columns": [
                        "genre",
                        "hidden_gem_score"
                    ]
                }
            ],
            "score": "high"
        },
        {
            "name": "Critical Reception for Drama Content",
            "task_id": 3,
            "description": "Understand the proportion of 'Fresh' vs. 'Rotten' critical reception ratings specifically for content classified as 'drama'.",
            "steps": [
                {
                    "function": "filter",
                    "column_name": "genre",
                    "operator": "in",
                    "values": [
                        "drama"
                    ]
                },
                {
                    "function": "get_proportion",
                    "column_name": [
                        "critical_reception_rating"
                    ]
                }
            ],
            "score": "high"
        },
        {
            "name": "Evaluate Production House Performance by Critic Scores",
            "task_id": 4,
            "description": "Compare the average combined critic scores ('average_critic_score') across different production houses to gauge their critical success.",
            "steps": [
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "production_house"
                    ],
                    "columns_to_aggregate": [
                        "average_critic_score"
                    ],
                    "calculation": [
                        "mean"
                    ]
                }
            ],
            "score": "high"
        },
        {
            "name": "Annual Movie Awards Recognition Trends",
            "task_id": 5,
            "description": "Analyze the average total awards recognition for movies based on their Netflix release year to observe trends over time.",
            "steps": [
                {
                    "function": "filter",
                    "column_name": "series_or_movie",
                    "operator": "==",
                    "values": [
                        "movie"
                    ]
                },
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "netflix_release_year"
                    ],
                    "columns_to_aggregate": [
                        "total_awards_recognition"
                    ],
                    "calculation": [
                        "mean"
                    ]
                }
            ],
            "score": "high"
        },
        {
            "name": "Top-Grossing Genres by Box Office",
            "task_id": 6,
            "description": "Identify the top 10 genres with the highest total box office earnings to understand market revenue drivers.",
            "steps": [
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "genre"
                    ],
                    "columns_to_aggregate": [
                        "boxoffice"
                    ],
                    "calculation": [
                        "sum"
                    ]
                },
                {
                    "function": "get_top_or_bottom_N_entries",
                    "sort_by_column_name": "boxoffice",
                    "order": "top",
                    "number_of_entries": 10,
                    "return_columns": [
                        "genre",
                        "boxoffice"
                    ]
                }
            ],
            "score": "high"
        },
        {
            "name": "Hidden Gem Score Difference for Critically Acclaimed Content",
            "task_id": 7,
            "description": "Calculate descriptive statistics (mean, median, min, max) for the 'hidden_gem_score_difference' for content rated 'Fresh' by critics.",
            "steps": [
                {
                    "function": "filter",
                    "column_name": "critical_reception_rating",
                    "operator": "==",
                    "values": [
                        "Fresh"
                    ]
                },
                {
                    "function": "get_column_statistics",
                    "column_name": [
                        "hidden_gem_score_difference"
                    ],
                    "calculation": [
                        "mean",
                        "median",
                        "min",
                        "max"
                    ]
                }
            ],
            "score": "high"
        },
        {
            "name": "Content Runtime Distribution in US",
            "task_id": 8,
            "description": "Determine the proportional distribution of content runtime categories (Short, Medium, Long, Very Long) for content available in the United States.",
            "steps": [
                {
                    "function": "filter",
                    "column_name": "country_availability",
                    "operator": "in",
                    "values": [
                        "united states"
                    ]
                },
                {
                    "function": "get_proportion",
                    "column_name": [
                        "content_runtime_category"
                    ]
                }
            ],
            "score": "high"
        },
        {
            "name": "Top Languages for Series by Popularity-Adjusted IMDb Score",
            "task_id": 9,
            "description": "Identify the top 5 languages associated with series based on their total 'weighted_imdb_score', reflecting both rating and popularity.",
            "steps": [
                {
                    "function": "filter",
                    "column_name": "series_or_movie",
                    "operator": "==",
                    "values": [
                        "series"
                    ]
                },
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "languages"
                    ],
                    "columns_to_aggregate": [
                        "weighted_imdb_score"
                    ],
                    "calculation": [
                        "sum"
                    ]
                },
                {
                    "function": "get_top_or_bottom_N_entries",
                    "sort_by_column_name": "weighted_imdb_score",
                    "order": "top",
                    "number_of_entries": 5,
                    "return_columns": [
                        "languages",
                        "weighted_imdb_score"
                    ]
                }
            ],
            "score": "high"
        },
        {
            "name": "Average IMDb Score by View Rating",
            "task_id": 10,
            "description": "Compare the average IMDb scores across different content view ratings (e.g., R, PG-13, TV-MA) to assess audience reception by rating category.",
            "steps": [
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "view_rating"
                    ],
                    "columns_to_aggregate": [
                        "imdb_score"
                    ],
                    "calculation": [
                        "mean"
                    ]
                }
            ],
            "score": "high"
        }
    ]
}