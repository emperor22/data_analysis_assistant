    "common_column_combination": [
        {
            "name": "average_critic_score",
            "description": "Calculates the average score from Rotten Tomatoes and Metacritic, providing a combined critic reception metric.",
            "operation": {
                "source_columns": [
                    "rotten_tomatoes_score",
                    "metacritic_score"
                ],
                "expression": "(rotten_tomatoes_score + metacritic_score) / 2"
            }
        },
        {
            "name": "total_awards_recognition",
            "description": "Sums the number of awards received and nominations to get a total count of industry recognition.",
            "operation": {
                "source_columns": [
                    "awards_received",
                    "awards_nominated_for"
                ],
                "expression": "awards_received + awards_nominated_for"
            }
        },
        {
            "name": "weighted_imdb_score",
            "description": "Calculates a weighted IMDb score by multiplying the IMDb score by the number of IMDb votes, indicating popularity adjusted by rating.",
            "operation": {
                "source_columns": [
                    "imdb_score",
                    "imdb_votes"
                ],
                "expression": "imdb_score * imdb_votes"
            }
        },
        {
            "name": "hidden_gem_score_difference",
            "description": "Measures the difference between the hidden gem score and the standard IMDb score, which could indicate undervalued content.",
            "operation": {
                "source_columns": [
                    "hidden_gem_score",
                    "imdb_score"
                ],
                "expression": "hidden_gem_score - imdb_score"
            }
        },
        {
            "name": "critic_score_variance",
            "description": "Measures the variance between Rotten Tomatoes and Metacritic scores to highlight critical disagreement.",
            "operation": {
                "source_columns": [
                    "rotten_tomatoes_score",
                    "metacritic_score"
                ],
                "expression": "abs(rotten_tomatoes_score - metacritic_score)"
            }
        }
    ],
    "common_column_cleaning_or_transformation": [
        {
            "name": "netflix_release_year",
            "description": "Extracts the year from the Netflix release date for temporal analysis.",
            "operation": {
                "type": "date_op",
                "source_column": "netflix_release_date",
                "function": "YEAR"
            }
        },
        {
            "name": "content_runtime_category",
            "description": "Categorizes content runtime into simplified groups (Short, Medium, Long, Very Long) for easier filtering.",
            "operation": {
                "type": "map",
                "source_column": "runtime",
                "mapping": {
                    "< 30 minutes": "Short",
                    "30-60 mins": "Medium",
                    "1-2 hour": "Long",
                    "> 2 hrs": "Very Long",
                    "nan": "Unknown"
                }
            }
        },
        {
            "name": "imdb_score_tier",
            "description": "Bins the IMDb score into qualitative tiers (Low, Medium, High) to categorize content quality.",
            "operation": {
                "type": "map_range",
                "source_column": "imdb_score",
                "ranges": [
                    {
                        "range": "0-5.9",
                        "label": "Low"
                    },
                    {
                        "range": "6.0-7.4",
                        "label": "Medium"
                    },
                    {
                        "range": "7.5-10.0",
                        "label": "High"
                    }
                ]
            }
        },
        {
            "name": "critical_reception_rating",
            "description": "Maps Rotten Tomatoes scores into simplified 'Fresh' or 'Rotten' categories based on common critical thresholds.",
            "operation": {
                "type": "map_range",
                "source_column": "rotten_tomatoes_score",
                "ranges": [
                    {
                        "range": "0-59",
                        "label": "Rotten"
                    },
                    {
                        "range": "60-100",
                        "label": "Fresh"
                    }
                ]
            }
        },
        {
            "name": "combined_audience_score",
            "description": "Calculates an average of IMDb and Hidden Gem scores to represent a general audience quality assessment.",
            "operation": {
                "type": "math_op",
                "source_columns": [
                    "imdb_score",
                    "hidden_gem_score"
                ],
                "expression": "(imdb_score + hidden_gem_score) / 2"
            }
        }
    ]
}