    {
      "name": "Identify Loans with Interest Rates Above 20%",
      "task_id": 10,
      "description": "Filter for high-interest loans to analyze their characteristics, such as purpose and default rate.",
      "steps": [
        {
          "function": "filter",
          "column_name": "int_rate",
          "operator": ">",
          "values": [
            20.0
          ]
        }
      ],
      "score": 0.89
