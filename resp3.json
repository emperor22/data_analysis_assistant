  "common_column_combination": [
    {
      "name": "debt_to_income_ratio",
      "description": "Calculate the debt-to-income ratio to assess a borrower's financial leverage.",
      "operation": {
        "source_columns": [
          "loan_amnt",
          "annual_inc"
        ],
        "expression": "loan_amnt / annual_inc"
      }
    }
  ],
  "common_column_cleaning_or_transformation": [
    {
      "name": "is_default",
      "description": "Create a binary flag for loan default status for modeling.",
      "operation": {
        "type": "map",
        "source_column": "loan_status",
        "mapping": {
          "charged off": 1,
          "fully paid": 0,
          "current": 0
        }
      }
    },
    {
      "name": "income_group",
      "description": "Categorize annual income into descriptive bins for segmentation.",
      "operation": {
        "type": "map_range",
        "source_column": "annual_inc",
        "ranges": [
          {
            "range": "0-40000",
            "label": "Low"
          },
          {
            "range": "40001-80000",
            "label": "Medium"
          },
          {
            "range": "80001-inf",
            "label": "High"
          }
        ]
      }
    },
    {
      "name": "term_length_category",
      "description": "Categorize loan term into 'Short' and 'Long' for easier analysis.",
      "operation": {
        "type": "map",
        "source_column": "term",
        "mapping": {
          "36": "Short Term",
          "60": "Long Term"
        }
      }
    }
  ],
  "common_tasks": [
    {
      "name": "Analyze Default Rate by Loan Purpose",
      "task_id": 1,
      "description": "Determine which loan purposes have the highest rate of being 'charged off' to identify high-risk categories.",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": [
            "purpose",
            "loan_status"
          ],
          "columns_to_aggregate": [
            "id"
          ],
          "calculation": [
            "size"
          ]
        }
      ],
      "score": 0.98
    },
    {
      "name": "Compare Average Interest Rate by Home Ownership",
      "task_id": 2,
      "description": "Understand if there is a difference in the average interest rate offered to borrowers based on their home ownership status.",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": [
            "home_ownership"
          ],
          "columns_to_aggregate": [
            "int_rate"
          ],
          "calculation": [
            "mean"
          ]
        }
      ],
      "score": 0.95
    },
    {
      "name": "Find Top 5 Loan Purposes by Average Loan Amount",
      "task_id": 3,
      "description": "Identify which loan purposes typically involve the largest loan amounts.",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": [
            "purpose"
          ],
          "columns_to_aggregate": [
            "loan_amnt"
          ],
          "calculation": [
            "mean"
          ]
        },
        {
          "function": "get_top_or_bottom_N_entries",
          "sort_by_column_name": "loan_amnt",
          "order": "top",
          "number_of_entries": 5,
          "return_columns": [
            "purpose",
            "loan_amnt"
          ]
        }
      ],
      "score": 0.92
    },
    {
      "name": "Analyze Loan Status Distribution",
      "task_id": 4,
      "description": "Get an overview of the proportion of loans that are fully paid, charged off, or current.",
      "steps": [
        {
          "function": "get_proportion",
          "column_name": [
            "loan_status"
          ],
          "values": []
        }
      ],
      "score": 0.99
    },
    {
      "name": "Examine Income of Defaulted vs. Paid Loans",
      "task_id": 5,
      "description": "Compare the median annual income of borrowers whose loans were charged off versus those who fully paid their loans.",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": [
            "loan_status"
          ],
          "columns_to_aggregate": [
            "annual_inc"
          ],
          "calculation": [
            "median"
          ]
        }
      ],
      "score": 0.96
    },
    {
      "name": "Investigate Default Rate by Loan Term",
      "task_id": 6,
      "description": "Determine if longer-term loans (60 months) have a different default rate compared to shorter-term loans (36 months).",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": [
            "term"
          ],
          "columns_to_aggregate": [],
          "calculation": []
        },
        {
          "function": "get_proportion",
          "column_name": [
            "loan_status"
          ],
          "values": []
        }
      ],
      "score": 0.93
    },
    {
      "name": "Find Top 10 Highest Income Borrowers Who Defaulted",
      "task_id": 7,
      "description": "Identify the top 10 borrowers with the highest annual income who still ended up with a 'charged off' loan status.",
      "steps": [
        {
          "function": "filter",
          "column_name": "loan_status",
          "operator": "in",
          "values": [
            "charged off"
          ]
        },
        {
          "function": "get_top_or_bottom_N_entries",
          "sort_by_column_name": "annual_inc",
          "order": "top",
          "number_of_entries": 10,
          "return_columns": [
            "id",
            "annual_inc",
            "loan_amnt"
          ]
        }
      ],
      "score": 0.88
    },
    {
      "name": "Analyze Average Total Payment by Verification Status",
      "task_id": 8,
      "description": "Compare the average total payment made by borrowers with different income verification statuses.",
      "steps": [
        {
          "function": "groupby",
          "columns_to_group_by": [
            "verification_status"
          ],
          "columns_to_aggregate": [
            "total_pymnt"
          ],
          "calculation": [
            "mean"
          ]
        }
      ],
      "score": 0.85
    },
    {
      "name": "Calculate Statistics for Loan Amount",
      "task_id": 9,
      "description": "Get key descriptive statistics (mean, median, min, max) for the loan amount across the entire dataset.",
      "steps": [
