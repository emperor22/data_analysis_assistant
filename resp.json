    "common_column_combination": [
        {
            "name": "total_interest_paid",
            "description": "Calculate the total interest paid over the loan's lifetime.",
            "operation": {
                "source_columns": [
                    "total_pymnt",
                    "loan_amnt"
                ],
                "expression": "total_pymnt - loan_amnt"
            }
        },
        {
            "name": "loan_profitability_ratio",
            "description": "Calculate the ratio of total interest paid to the original loan amount, indicating profitability.",
            "operation": {
                "source_columns": [
                    "total_pymnt",
                    "loan_amnt"
                ],
                "expression": "(total_pymnt - loan_amnt) / loan_amnt"
            }
        },
        {
            "name": "initial_debt_to_income_ratio",
            "description": "Calculate the initial ratio of loan amount to annual income, indicating the borrower's debt burden.",
            "operation": {
                "source_columns": [
                    "loan_amnt",
                    "annual_inc"
                ],
                "expression": "loan_amnt / annual_inc"
            }
        }
    ],
    "common_column_cleaning_or_transformation": [
        {
            "name": "loan_to_income_ratio",
            "description": "Calculate the ratio of loan amount to annual income, indicating financial burden.",
            "operation": {
                "type": "math_op",
                "source_columns": [
                    "loan_amnt",
                    "annual_inc"
                ],
                "expression": "loan_amnt / annual_inc"
            }
        },
        {
            "name": "term_duration",
            "description": "Categorize loan term into short or long duration.",
            "operation": {
                "type": "map",
                "source_column": "term",
                "mapping": {
                    "36": "Short Term",
                    "60": "Long Term"
                }
            }
        },
        {
            "name": "annual_income_bracket",
            "description": "Classify annual income into low, medium, or high brackets for segmentation.",
            "operation": {
                "type": "map_range",
                "source_column": "annual_inc",
                "ranges": [
                    {
                        "range": "0-40000",
                        "label": "Low"
                    },
                    {
                        "range": "40001-80000",
                        "label": "Medium"
                    },
                    {
                        "range": "80001-inf",
                        "label": "High"
                    }
                ]
            }
        },
        {
            "name": "loan_issue_period_category",
            "description": "Categorize loan issue date into defined periods based on relative months.",
            "operation": {
                "type": "map_range",
                "source_column": "issue_d",
                "ranges": [
                    {
                        "range": "-inf--10",
                        "label": "Oldest_Period"
                    },
                    {
                        "range": "-9--8",
                        "label": "Mid_Period"
                    },
                    {
                        "range": "-7-inf",
                        "label": "Recent_Period"
                    }
                ]
            }
        },
        {
            "name": "monthly_payment_burden",
            "description": "Ratio of monthly installment to monthly income, indicating monthly financial strain.",
            "operation": {
                "type": "math_op",
                "source_columns": [
                    "installment",
                    "annual_inc"
                ],
                "expression": "installment / (annual_inc / 12)"
            }
        }
    ],
    "common_tasks": [
        {
            "name": "Analyze average loan amount by loan purpose",
            "task_id": 1,
            "description": "Determine the average loan amount requested for each loan purpose.",
            "steps": [
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "purpose"
                    ],
                    "columns_to_aggregate": [
                        "loan_amnt"
                    ],
                    "calculation": [
                        "mean"
                    ]
                }
            ],
            "score": 0.9
        },
        {
            "name": "Proportion of charged-off loans",
            "task_id": 2,
            "description": "Calculate the proportion of loans that have been charged off.",
            "steps": [
                {
                    "function": "get_proportion",
                    "column_name": [
                        "loan_status"
                    ],
                    "values": [
                        "charged off"
                    ]
                }
            ],
            "score": 0.95
        },
        {
            "name": "Top 5 loan purposes by total payments received",
            "task_id": 3,
            "description": "Identify which loan purposes have generated the highest total payments.",
            "steps": [
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "purpose"
                    ],
                    "columns_to_aggregate": [
                        "total_pymnt"
                    ],
                    "calculation": [
                        "sum"
                    ]
                },
                {
                    "function": "get_top_or_bottom_N_entries",
                    "sort_by_column_name": "total_pymnt",
                    "order": "top",
                    "number_of_entries": 5,
                    "return_columns": [
                        "purpose",
                        "total_pymnt"
                    ]
                }
            ],
            "score": 0.88
        },
        {
            "name": "Loan status distribution for different home ownership types",
            "task_id": 4,
            "description": "Examine how loan status varies across different home ownership categories.",
            "steps": [
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "home_ownership",
                        "loan_status"
                    ],
                    "columns_to_aggregate": [
                        "id"
                    ],
                    "calculation": [
                        "count"
                    ]
                }
            ],
            "score": 0.92
        },
        {
            "name": "Compare average interest rate for fully paid vs. charged off loans",
            "task_id": 5,
            "description": "Determine if there's a significant difference in average interest rates between fully paid and charged off loans.",
            "steps": [
                {
                    "function": "filter",
                    "column_name": "loan_status",
                    "operator": "in",
                    "values": [
                        "fully paid",
                        "charged off"
                    ]
                },
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "loan_status"
                    ],
                    "columns_to_aggregate": [
                        "int_rate"
                    ],
                    "calculation": [
                        "mean"
                    ]
                }
            ],
            "score": 0.94
        },
        {
            "name": "Identify loans with high interest rates for specific purposes",
            "task_id": 6,
            "description": "Find loans with an interest rate above a certain threshold, grouped by purpose.",
            "steps": [
                {
                    "function": "filter",
                    "column_name": "int_rate",
                    "operator": ">",
                    "values": [
                        15.0
                    ]
                },
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "purpose"
                    ],
                    "columns_to_aggregate": [
                        "id"
                    ],
                    "calculation": [
                        "count"
                    ]
                },
                {
                    "function": "get_top_or_bottom_N_entries",
                    "sort_by_column_name": "id",
                    "order": "top",
                    "number_of_entries": 5,
                    "return_columns": [
                        "purpose",
                        "id"
                    ]
                }
            ],
            "score": 0.85
        },
        {
            "name": "Analyze average loan amount based on verification status",
            "task_id": 7,
            "description": "Investigate how the average loan amount differs based on the borrower's income verification status.",
            "steps": [
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "verification_status"
                    ],
                    "columns_to_aggregate": [
                        "loan_amnt"
                    ],
                    "calculation": [
                        "mean"
                    ]
                }
            ],
            "score": 0.87
        },
        {
            "name": "Examine annual income distribution for charged off loans",
            "task_id": 8,
            "description": "Understand the typical annual income range for borrowers whose loans were charged off.",
            "steps": [
                {
                    "function": "filter",
                    "column_name": "loan_status",
                    "operator": "in",
                    "values": [
                        "charged off"
                    ]
                },
                {
                    "function": "get_column_statistics",
                    "column_name": [
                        "annual_inc"
                    ],
                    "calculation": [
                        "mean",
                        "median",
                        "min",
                        "max"
                    ]
                }
            ],
            "score": 0.9
        },
        {
            "name": "Identify average installment by loan term",
            "task_id": 9,
            "description": "Determine the average monthly installment for loans with different terms (e.g., 36 months vs. 60 months).",
            "steps": [
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "term"
                    ],
                    "columns_to_aggregate": [
                        "installment"
                    ],
                    "calculation": [
                        "mean"
                    ]
                }
            ],
            "score": 0.86
        },
        {
            "name": "Explore loan status trends across different issue periods",
            "task_id": 10,
            "description": "Analyze the distribution of loan statuses for loans issued in different relative time periods.",
            "steps": [
                {
                    "function": "groupby",
                    "columns_to_group_by": [
                        "loan_issue_period_category",
                        "loan_status"
                    ],
                    "columns_to_aggregate": [
                        "id"
                    ],
                    "calculation": [
                        "count"
                    ]
                }
            ],
            "score": 0.89
        }
    ]
}